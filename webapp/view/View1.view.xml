                                    <Column>
                                        <Text text="{i18n>date}" />
                                    </Column>
                                    <Column>
                                        <Text text="{i18n>currency}" />
                                    </Column>
                                </columns>
                                <items>
                                    <ColumnListItem>
                                        <cells>
                                            <Text text="{helper>date}" />
                                            <Text text="{helper>currency}" />
                                        </cells>
                                    </ColumnListItem>
                                </items>
                            </Table>
